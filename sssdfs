--//Rage
Rage:Dropdown("Spin Bot Type", {"None", "Normal", "Jitter", "Anti Lock"}, function(value)
if value == "Normal" then
    getgenv().spinn = true
getgenv().speedd = 90

game:GetService("RunService").Heartbeat:Connect(function()
    if getgenv().spinn then
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.Angles(0,math.random(getgenv().speedd), 0)
    end
end)
elseif value == "Jitter" then
    Jitter = true

function JitterAA()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
    CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position) *
    CFrame.Angles(0, math.rad(60) + math.rad((math.random(1, 2) == 1 and math.random(30, 90) or -math.random(30, 90))), 0)
end

game:GetService("RunService").Heartbeat:Connect(function()
if Jitter == true then
    JitterAA()
    else
end
end)
elseif value == "Anti Lock" then
    getgenv().spin = true
getgenv().speed = 90

function Spin()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * 
    CFrame.Angles(0,math.random(getgenv().speed), 0)
end
game:GetService("RunService").Heartbeat:Connect(function()
if getgenv().spin == true then
local antiurmum = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -100, 0)
game:GetService("RunService").RenderStepped:Wait()
game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = antiurmum
end
if getgenv().spin == true then
Spin()
end
end)
elseif value == "None" then
    getgenv().spinn = false
    getgenv().spin = false
    Jitter = false
    end
end)
Rage:Toggle("CFrame Speed", function(cff)
    getgenv().SpeedEnabled = cff
end)
getgenv().SpeedEnabled = false
game:GetService("RunService").heartbeat:Connect(function()
if getgenv().SpeedEnabled then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = 
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + 
game.Players.LocalPlayer.Character.Humanoid.MoveDirection * getgenv().SpeedAmount
end
end)
Rage:Slider("CFrame Speed Value", 0, 1000, 10, function(v)
getgenv().SpeedAmount = v / 250
end)
Rage:Toggle("Anti Stomp", function(v)
    getgenv().AntiStomp = v
end)
getgenv().AntiStomp = false
game:GetService("RunService").Heartbeat:Connect(function() 
if getgenv().AntiStomp then
            if game.Players.LocalPlayer.Character.Humanoid.Health <= 5 then
                for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v:Destroy()
                    end
                end
            end
end
end)
Rage:Toggle("No Jump CoolDown", function(cooldown)
if cooldown == true then
game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = false
elseif cooldown == false then
game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
end
end)
Rage:Slider("HipHeight Value", 0, 100, 10, function(v)
    game.Players.LocalPlayer.Character.Humanoid.HipHeight = v
end)
Rage:Button("Fix Hip Height", function()
game.Players.LocalPlayer.Character.Humanoid.HipHeight = 1.85
end)

