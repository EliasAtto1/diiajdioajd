Legit:Dropdown("Anti Aim Type", {"None", "Ground", "Sky", "Spin Ground", "Spin Sky", "Horizontal", "Confusion", "Blatant AA"}, function(value)
    if value == "Ground" then
        getgenv().bot = true 
        getgenv().emt = -100
        
        game:GetService("RunService").heartbeat:Connect(function()
        if getgenv().bot ~= false then
        local antiurmum = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, getgenv().emt, 0)
            game:GetService("RunService").RenderStepped:Wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = antiurmum
        end
        end)
        elseif value == "Sky" then
        getgenv().bot2 = true 
        getgenv().emt = 100
        
        game:GetService("RunService").heartbeat:Connect(function()
        if getgenv().bot2 ~= false then
        local antiurmum = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, getgenv().emt, 0)
            game:GetService("RunService").RenderStepped:Wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = antiurmum
        end
        end)
        elseif value == "Spin" then
        getgenv().spin = true
        getgenv().speed = 100
        
        function Spin()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * 
            CFrame.Angles(0,math.random(getgenv().speed), 0)
        end
        game:GetService("RunService").Heartbeat:Connect(function()
        if getgenv().spin == true then
        local antiurmum = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, -100, 0)
        game:GetService("RunService").RenderStepped:Wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = antiurmum
        end
        if getgenv().spin == true then
        Spin()
        end
        end)
    elseif value == "Spin Sky" then
        getgenv().spinn = true
        getgenv().speedd = 100
        
        function Spin()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * 
            CFrame.Angles(0,math.random(getgenv().speedd), 0)
        end
        game:GetService("RunService").Heartbeat:Connect(function()
        if getgenv().spinn == true then
        local antiurmum = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 90, 0)
        game:GetService("RunService").RenderStepped:Wait()
        game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = antiurmum
        end
        if getgenv().spinn == true then
        Spin()
        end
        end)
    elseif value == "Horizontal" then
getgenv().HorizontalAA = true
getgenv().HorizontalAAVelocity = 120

game:GetService("RunService").heartbeat:Connect(
function()
    if getgenv().HorizontalAA == true then 
    local velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(
    0,0,getgenv().HorizontalAAVelocity
    ) 
    game:GetService("RunService").RenderStepped:Wait()
    game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = velocity
    end 
end)
elseif value == "Confusion" then
getgenv().ConfusionAA = true
getgenv().ConfusionAAamount = 2

game:GetService"RunService".RenderStepped:Connect(function()
        if getgenv().ConfusionAA == true then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = 
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * 
        CFrame.new(
        math.random(1, 2) == 1 and getgenv().ConfusionAAamount or -getgenv().ConfusionAAamount, 0, 0
        )
        end
end)
    elseif value == "Blatant AA" then
        getgenv().bantiaim = true

        local Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        while getgenv().bantiaim == true  do
            task.wait()
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = (CFrame.new(Position) + Vector3.new(math.random(-15, 15), math.random(-15, 15), math.random(-15, 15))) * CFrame.Angles(math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)), math.rad(math.random(-180, 180)))
        end
        elseif value == "None" then
        getgenv().bot = false
        getgenv().bot2 = false
        getgenv().spin = false
        getgenv().bantiaim = false  
        getgenv().spinn = false
        getgenv().HorizontalAA = false
        getgenv().ConfusionAA = false
        end
end)







